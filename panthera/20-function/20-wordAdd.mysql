CREATE OR REPLACE FUNCTION `wordAdd`(
  ILANG INT UNSIGNED
) RETURNS BIGINT UNSIGNED MODIFIES SQL DATA
BEGIN
    DECLARE word_id BIGINT UNSIGNED; 
    DECLARE lresult SMALLINT; 
    INSERT INTO `words`() VALUES ();
    SELECT LAST_INSERT_ID() INTO word_id;
    SELECT `langAddToWord`( ILANG, word_id ) INTO lresult;
    RETURN word_id;
END;
